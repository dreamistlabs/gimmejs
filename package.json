{
  "name": "gimmejs",
  "version": "0.0.2",
  "description": "",
  "keywords": [],
  "author": "Johnny Hu",
  "license": "MIT",
  "main": "./dist/gimme.node.js",
  "scripts": {
    "build": "webpack",
    "coveralls": "npm run test:coverage -- --coverageReporters=text-lcov | coveralls",
    "prepublishOnly": "npm run build",
    "release:major": "changelog -M && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version major && git push origin && git push origin --tags",
    "release:minor": "changelog -m && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version minor && git push origin && git push origin --tags",
    "release:patch": "changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version patch && git push origin && git push origin --tags",
    "start": "webpack-dev-server --mode development --open",
    "test": "jest --colors --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --colors --verbose --watch",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dreamistlabs/gimmejs.git"
  },
  "bugs": {
    "url": "https://github.com/dreamistlabs/gimmejs/issues"
  },
  "homepage": "https://github.com/dreamistlabs/gimmejs#readme",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "packages"
    ],
    "setupFilesAfterEnv": [
      "jest-expect-message"
    ],
    "testMatch": [
      "<rootDir>/__tests__/*.test.(js|ts)"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.15.1",
    "generate-changelog": "^1.7.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "jest-expect-message": "^1.0.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {}
}
