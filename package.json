{
  "name": "gimmejs",
  "version": "0.0.2",
  "description": "",
  "keywords": [],
  "author": "Johnny Hu",
  "license": "MIT",
  "main": "./dist/gimme.node.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:next": "next build",
    "build:webpack": "webpack",
    "coveralls": "npm run test:coverage -- --coverageReporters=text-lcov | coveralls",
    "export": "npm run build:next && next export -o docs && cp .nojekyll docs/",
    "prepublishOnly": "npm run build:webpack",
    "release:major": "changelog -M && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version major && git push origin && git push origin --tags",
    "release:minor": "changelog -m && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version minor && git push origin && git push origin --tags",
    "release:patch": "changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version patch && git push origin && git push origin --tags",
    "start": "next start",
    "test": "jest --colors --verbose",
    "test:coverage": "jest --coverage",
    "watch": "run-p watch:webpack",
    "watch:next": "next dev",
    "watch:test": "jest --colors --verbose --watch",
    "watch:webpack": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dreamistlabs/gimmejs.git"
  },
  "bugs": {
    "url": "https://github.com/dreamistlabs/gimmejs/issues"
  },
  "homepage": "https://github.com/dreamistlabs/gimmejs#readme",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "packages"
    ],
    "setupFilesAfterEnv": [
      "jest-expect-message"
    ],
    "testMatch": [
      "<rootDir>/__tests__/*.test.(js|ts)"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-preset-minify": "^0.5.0",
    "clean-webpack-plugin": "^2.0.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.15.1",
    "generate-changelog": "^1.7.1",
    "jest": "^24.1.0",
    "jest-expect-message": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "next": "^8.1.0",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
