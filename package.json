{
  "name": "gimmejs",
  "version": "0.0.2",
  "description": "",
  "keywords": [],
  "author": "Johnny Hu",
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rm -rf dist/* && ./node_modules/.bin/babel src -d dist -s inline",
    "coveralls": "npm run test && cat ./tests/coverage/lcov.info | coveralls",
    "prepublishOnly": "npm run build",
    "release:major": "changelog -M && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version major && git push origin && git push origin --tags",
    "release:minor": "changelog -m && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version minor && git push origin && git push origin --tags",
    "release:patch": "changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version patch && git push origin && git push origin --tags",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "watch": "./node_modules/.bin/babel src -d dist --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dreamistlabs/gimmejs.git"
  },
  "bugs": {
    "url": "https://github.com/dreamistlabs/gimmejs/issues"
  },
  "homepage": "https://github.com/dreamistlabs/gimmejs#readme",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "packages"
    ],
    "setupFilesAfterEnv": [
      "jest-expect-message"
    ],
    "testMatch": [
      "<rootDir>/(tests/*.test.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx))"
    ],
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-jest": "^24.1.0",
    "babel-preset-minify": "^0.5.0",
    "coveralls": "^2.13.1",
    "generate-changelog": "^1.7.1",
    "jest": "^24.1.0",
    "jest-expect-message": "^1.0.2",
    "travis-ci": "^2.1.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "gimmejs-currency": "^1.0.0",
    "gimmejs-name": "0.0.2"
  }
}
