{
  "name": "root",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dreamistlabs/gimmejs.git"
  },
  "author": "Johnny Hu <johnny@dreamistlabs.com>",
  "bugs": {
    "url": "https://github.com/dreamistlabs/gimmejs/issues"
  },
  "homepage": "https://github.com/dreamistlabs/gimmejs#readme",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lerna:bootstrap": "lerna bootstrap",
    "lerna:build": "lerna run build",
    "lerna:test": "lerna run test",
    "test": "nyc ava",
    "test:watch": "ava --watch",
    "tslint": "tslint -c tslint.json -p tsconfig.json **/*.ts",
    "watch": "webpack --config webpack.config.js -w"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm test"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-destructuring": "^7.3.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "ava": "^1.2.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "coveralls": "^2.13.1",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.1.0",
    "husky": "^3.0.9",
    "lerna": "^3.13.0",
    "nyc": "^14.1.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "gimmejs-currency": "^1.0.0",
    "gimmejs-name": "0.0.2"
  }
}
