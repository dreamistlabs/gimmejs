  "bugs": {
    "url": "https://github.com/dreamistlabs/gimmejs-error/issues"
  },
  "homepage": "https://github.com/dreamistlabs/gimmejs-error#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.1",
    "coveralls": "^2.13.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0"
  }
}
