{
  "name": "gimmejs-error",
  "version": "0.0.1",
  "description": "A simple, custom Error class meant to be used in gimmeJS projects",
  "main": "./lib/gimmejs-error.js",
  "engines": {
    "node": ">=4.2.4"
  },
  "scripts": {
    "compile": "babel src --out-dir lib --source-maps inline",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec test/*",
